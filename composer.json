{
    "name": "cemal/php-playground",
    "description": "PHP playground with MySQL database using Laravel Illuminate",
    "type": "project",
    "require": {
        "brick/json-mapper": "^0.1.1",
        "illuminate/database": "^11.0",
        "illuminate/events": "^11.0",
        "illuminate/http": "^11.46",
        "jane-php/json-schema": "^7.9",
        "robmorgan/phinx": "^0.16.10",
        "spatie/ray": "^1.42",
        "spatie/regex": "^3.1",
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
